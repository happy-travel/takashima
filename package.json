{
  "name": "com.happytravel.takashima",
  "version": "0.5.1",
  "private": true,
  "dependencies": {
    "antd": "4.24.6",
    "iconsax-react": "0.0.8",
    "mobx": "6.7.0",
    "mobx-react": "7.6.0",
    "oidc-client": "1.11.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-json-tree": "0.17.0",
    "react-router-dom": "6.6.1"
  },
  "devDependencies": {
    "@parcel/transformer-less": "2.8.2",
    "@parcel/transformer-sass": "2.8.2",
    "parcel": "2.8.2",
    "parcel-reporter-static-files-copy": "1.4.0",
    "parcel-resolver-ignore": "2.1.3",
    "prettier": "2.7.1",
    "process": "0.11.10"
  },
  "scripts": {
    "start": "parcel src/index.html --port 4000",
    "build": "parcel build src/index.html --dist-dir build",
    "prettier": "prettier ./src/* --write"
  },
  "staticFiles": {
    "staticPath": "public"
  },
  "browserslist": [
    "> 0.5%, last 2 versions, not dead"
  ],
  "parcelIgnore": [
    "favicon.ico",
    "images/.+"
  ],
  "alias": {
    "core": "~/src/core",
    "stores": "~/src/stores",
    "htcore": "~/src/htcore",
    "pages": "~/src/pages",
    "common": "~/src/common",
    "components": "~/src/components"
  }
}
